@{
    var DiaChiManHinhChucNang = "";
    if (IsPost)
    {
        var MaSo = Request["MaSo"];
        if (MaSo == "MH_ChaoKhachThamQuan")
        {
            DiaChiManHinhChucNang = "MH_ChaoKhachThamQuan";
        }
        else if (MaSo == "MH_ChaoKhachHang")
        {
            DiaChiManHinhChucNang = "MH_ChaoKhachHang";

        }
        else if (MaSo == "MH_ChaoNhanVien")
        {
            DiaChiManHinhChucNang = "MH_ChaoNhanVien";
        }
        else
        {
            DiaChiManHinhChucNang = "MH_ChaoQuanLy";
        }
    }
    if (DiaChiManHinhChucNang != "")
    {
        Response.Redirect(DiaChiManHinhChucNang);     
    }
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous">

</head>

<body style="font-family:Arial">
    <form method="post">
        <div class="container-fluid">
            <div class="btn btn-outline-primary">Chọn 1 lời chào và click đồng ý</div> <br /><br />
            <div>
                <input type="radio" name="MaSo" value="MH_ChaoKhachThamQuan">Chào Khách tham quan<br />
                <input type="radio" name="MaSo" value="MH_ChaoKhachHang">Chào Khách hàng<br />
                <input type="radio" name="MaSo" value="MH_ChaoNhanVien">Chào Nhân viên<br />
                <input type="radio" name="MaSo" value="MH_ChaoQuanLy">Chào Quản lý<br />
                <button class="btn btn-danger" type="submit">Đồng ý</button>
            </div>
        </div>
    </form>
</body>
</html>
